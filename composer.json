{
    "name": "ccmbenchmark/ting-api-platform",
    "description": "Api platform bridge for Ting",
    "license": "Apache-2.0",
    "type": "library",
    "require": {
        "php": ">=8.1",
        "api-platform/core": "~3.0",
        "ccmbenchmark/ting_bundle": "^3.6",
        "ccmbenchmark/ting": "dev-fix/hydrator/relational as 3.7.0"
    },
    "require-dev": {
        "phpstan/extension-installer": "^1.2",
        "phpstan/phpstan": "^1.0",
        "phpstan/phpstan-symfony": "^1.0"
    },
    "autoload": {
        "files": [
            "src/Safe/pcre.php"
        ],
        "psr-4": {
            "CCMBenchmark\\Ting\\ApiPlatform\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "CCMBenchmark\\Ting\\ApiPlatform\\Test\\": "test/"
        }
    },
    "config": {
        "allow-plugins": {
            "phpstan/extension-installer": true
        }
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/Lctrs/ting.git"
        }
    ]
}
